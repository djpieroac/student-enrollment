{
  "name": "boilerplate_backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "tsc": "npx tsc --noEmit",
    "migrate": "prisma migrate dev --name init --schema=prisma/schema.prisma",
    "generate": "npx prisma generate --schema=prisma/schema.prisma",
    "db:push": "prisma db push --schema=prisma/schema.prisma",
    "studio": "prisma studio --schema=prisma/schema.prisma",
    "reset": "prisma migrate reset --force --schema=prisma/schema.prisma",
    "seed": "npm run generate && ts-node prisma/seed.ts",
    "dev:start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev": "npm run generate && npm run dev:start",
    "format": "biome format --write src",
    "lint": "biome check --write src",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "author": "djpieroac",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/cookie-parser": "^1.4.9",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.4.0",
    "prisma": "^6.16.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1"
  }
}
